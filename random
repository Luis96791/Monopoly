#include <stdlib.h>
#include <iostream>

using namespace std;

void timeUno(){
    int num1;
    srand(time(NULL));
    for(int c =1;c<=6;c++){
        num1 = 1 +rand()%(7-1);
    }
    cout<<num1;
}

void timeDos(){
    int num2;
    for(int h =1;h<=6;h++){
        num2 = 1 +rand()%(7-1);
    }
    cout<<num2;
}

int main(){

    timeUno();
    timeDos();
    return 0;
}
